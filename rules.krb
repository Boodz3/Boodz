has_Degree_equivelent
    foreach
        notany
            facts.tech_degree($_any)
        facts.tech_cert($field, $ccount)
        facts.tech_experience($field, $years)
        fr.equivDegree($field, $ryears)
        check $years >= $ryears
        $eyears = $years-$ryears
    assert 
        facts.QualifiedForTech($field, $eyears)

has_degree
