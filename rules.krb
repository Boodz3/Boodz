        fr.SE_Skill(General, $gen)
        facts.tech_skill(Lang, $lang)
        facts.tech_skill(Req, $req)
        facts.tech_skill(General, $gen)
    assert 
        facts.QualifiedForJob('Software Engineer Junior')

